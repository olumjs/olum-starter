{
  "name": "olum-app",
  "version": "0.1.0",
  "private": true,
  "description": "Olum App",
  "main": "index.js",
  "scripts": {
    "dev": "olum-compiler dev",
    "build": "olum-compiler build"
  },
  "author": "Olumjs",
  "dependencies": {
    "olum": "latest",
    "olum-router": "latest"
  },
  "devDependencies": {
    "olum-compiler": "latest"
  },
  "olum": {
    "port": 8000,
    "livereload": true,
    "fallback": "/",
    "comments": false,
    "hash": 5,
    "title": "Olum App",
    "polyfill": false,
    "assetAsModule": true,
    "https": false,
    "favicon": "./public/favicon.png",
    "template": "./public/index.html",
    "serviceWorker": false,
    "manifest": {
      "name": "Olum App - Progressive Web App",
      "short_name": "Olum App",
      "description": "Olum App",
      "background_color": "#ffffff",
      "publicPath": "./",
      "display": "standalone",
      "orientation": "any",
      "start_url": "index.html",
      "theme_color": "#ffffff",
      "icons": [
        {
          "src": "./public/manifest.png",
          "sizes": [
            96,
            128,
            192,
            256,
            384,
            512
          ]
        }
      ]
    }
  }
}
